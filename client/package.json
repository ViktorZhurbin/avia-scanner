    "react-calendar": "^2.19.2",
    "react-dom": "^16.12.0",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^6.0.1",
    "react-transition-group": "^2.9.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-starter-kit": "^0.4.3"
  },
