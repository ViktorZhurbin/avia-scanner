{
  "name": "avia-scanner",
  "main": "index.js",
  "browserslist": [
    "last 2 versions",
    "not < 1%",
    "not ie 11"
  ],
  "scripts": {
    "client": "webpack-dev-server --config configs/webpack.dev.js",
    "server": "nodemon ../index.js",
    "dev": "concurrently 'npm run server' 'npm run client'",
    "build": "webpack --config configs/webpack.prod.js",
    "stats": "npm run stats:build && npm run stats:analyse",
    "stats:build": "webpack --config configs/webpack.prod.js --profile --json > stats.json",
    "stats:analyse": "webpack-bundle-analyzer stats.json ./build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint-fix": "npm run lint:js -- --fix && npm run lint:css -- --fix",
    "lint:js": "eslint src/",
    "lint:css": "stylelint 'src/**/*.css'",
    "test:server": "cd .. && npm test",
    "test:client": "NODE_ICU_DATA=node_modules/full-icu jest --config=jest.config.json",
    "test": "npm run test:client && npm run test:server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": "npm run lint && npm test"
    }
  },
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViktorZhurbin/avia-scanner.git"
  },
  "dependencies": {
    "browser-cookies": "^1.2.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.13",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "full-icu": "^1.2.1",
    "locale-currency": "0.0.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-calendar": "^2.18.1",
    "react-dom": "^16.8.6",
    "react-outside-event": "^1.2.7",
    "react-redux": "^6.0.1",
    "react-svg-inline": "^2.1.1",
    "react-transition-group": "^2.9.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "redux-starter-kit": "^0.4.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "brotli-webpack-plugin": "*1.0.0",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "postcss-custom-properties": "^8.0.10",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.0",
    "raw-loader": "^1.0.0",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
