    "browser-cookies": "^1.2.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.18",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
