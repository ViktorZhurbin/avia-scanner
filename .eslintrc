{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "jest"
    ],
    "env": {
        "node": true,
        "es6": true,
        "jest/globals": true
    },
    "rules": {
        "no-console": "off"
    }
}